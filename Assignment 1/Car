Public class Car {

        /* move method takes an integer parameter "distance" */
        public void move(int distance) {
 
                if(distance > 0) {  
                  System.out.println("Moving");
                }

        }
        
  public static void main(String[] args) {
            
                Car c = new Car();

                /* calling move with distance greater than 0 */
                c.move(10);

                /* calling move with distance less than 0 
                this method will not print nothing
                */
                c.move(-10);
  }
}
